<h1>New Concert</h1>

<%= form_for(@concerts) do |f| %>
    <%= f.label :artists %>
    <%= f.text_field :artists %>

    <%= f.label :venue %>
    <%= f.text_field :venue %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.date_field :date, class: 'form-control' %>
    
    <%= f.submit %>

<% end %>