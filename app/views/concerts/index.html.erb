<h1>All Concerts</h1>
 
<table>
 
  <tbody>
    <% @concerts.map do |c|%>
      <tr>
        <td><% c.artists.map do |a| %></td>
          <td> <%= a.name %> </td>
        <% end %>
        <td><%= c.venue %></td>
        <td><%= c.city %></td>
        <td><%= c.state %></td>
        <td><%= c.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>
 
<br>
 
<%= link_to "New concert", new_concert_path %>